{
  "name": "lloc/gitphpcl",
  "description": "Generate your changelog from git with PHP",
  "require": {
    "php": ">=7.4"
  },
  "keywords": ["git","changelog"],
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Dennis Ploetner",
      "email": "re@lloc.de",
      "homepage": "http://lloc.de",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4" : {
      "lloc\\gitphpcl\\" : "src/"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^1.10",
    "squizlabs/php_codesniffer": "^3.7",
    "phpcompatibility/php-compatibility": "^9.3"
  }
}
